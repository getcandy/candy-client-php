{
    "name": "getcandy/candy-client-php",
    "description": "A set of tools to query the GetCandy API, written in PHP",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.1"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "GetCandy\\Client\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GetCandy\\Client\\GetCandyServiceProvider"
            ],
            "aliases": {
                "CandyClient" : "GetCandy\\Client\\Facades\\CandyClient"
            }
        }
    }
}
